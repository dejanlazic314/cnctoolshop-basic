{
  "name": "dejanlazic314/cnctoolshop-basic",
  "type": "contao-bundle",
  "description": "CNC Tool Shop Basic Bundle",
  "license": "proprietary",
  "homepage": "https://github.com/dejanlazic314/cnctoolshop-basic",
  "authors": [
    {
      "name": "Dejan Lazic",
      "email": "dejan.lazic314@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.3",
    "vonheldenundgestalten/contao-basic": "^5.3",
    "vonheldenundgestalten/contao-inserttags": "^5.3",
    "mvo/contao-group-widget": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Cnctoolshop\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao/manager-plugin": true,
      "php-http/discovery": false
    }
  },
  "extra": {
    "contao-manager-plugin": "Cnctoolshop\\ContaoManager\\Plugin"
  }
}
